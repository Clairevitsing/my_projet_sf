{% extends 'base.html.twig' %}


{% block title %}
	article
{% endblock %}

{% block body %}


	<ul>
		{% for article in articles %}
			<li>
				<a href="{{ path('article_item',{ id: article.id })}}">
					{{ article.name }}
				</a>
			</li>
			<li>
				<p>{{ article.content }}</p>
			</li>
		{% endfor %}
	</ul>
{% endblock %}
